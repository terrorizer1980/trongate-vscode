{
  "name": "trongate",
  "displayName": "Trongate",
  "description": "VS Code extension to compliment the Trongate ecosystem",
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.31.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Keymaps"
  ],
  "publisher": "jc-sf",
  "repository": {
    "type": "git",
    "url": "https://github.com/jakecastelli/trongate-vscode"
  },
  "icon": "assets/logo.png",
  "homepage": "https://trongate.io",
  "activationEvents": [
    "onStartupFinished",
    "onCommand:trongate.insertSnippet",
    "onCommand:trongate.newTrongate",
    "onCommand:trongate.selectNitroFramework"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": [
      {
        "title": "Trongate",
        "properties": {
          "trongate.userFrameworkOption": {
            "type": "string",
            "default": "Boostrap 4",
            "description": "User's nitro framework option"
          }
        }
      }
    ],
    "commands": [
      {
        "command": "trongate.insertSnippet",
        "title": "Select a front-end framework"
      },
      {
        "command": "trongate.newTrongate",
        "title": "New Trongate Module"
      },
      {
        "command": "trongate.selectNitroFramework",
        "title": "CSS Framework"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "trongate.newTrongate",
          "group": "trongateGroup@1",
          "when": "explorerResourceIsFolder"
        }
      ]
    },
    "snippets": [
      {
        "language": "html",
        "path": "./snippets/trongate/tg.json"
      },
      {
        "language": "php",
        "path": "./snippets/trongate/tg.json"
      },
      {
        "language": "html",
        "path": "./snippets/frontend/bootstrap.json"
      },
      {
        "language": "php",
        "path": "./snippets/frontend/bootstrap.json"
      },
      {
        "language": "html",
        "path": "./snippets/frontend/defiant.json"
      },
      {
        "language": "php",
        "path": "./snippets/frontend/defiant.json"
      },
      {
        "language": "html",
        "path": "./snippets/frontend/materialize.json"
      },
      {
        "language": "php",
        "path": "./snippets/frontend/materialize.json"
      },
      {
        "language": "html",
        "path": "./snippets/frontend/milligram.json"
      },
      {
        "language": "php",
        "path": "./snippets/frontend/milligram.json"
      },
      {
        "language": "html",
        "path": "./snippets/frontend/skeleton.json"
      },
      {
        "language": "php",
        "path": "./snippets/frontend/skeleton.json"
      }
    ],
    "keybindings": [
      {
        "mac": "ctrl+cmd+alt+/",
        "key": "ctrl+win+alt+/",
        "when": "editorTextFocus",
        "command": "trongate.selectNitroFramework"
      },
      {
        "key": "ctrl+win+alt+b",
        "command": "trongate.insertSnippet",
        "when": "editorTextFocus",
        "args": {
          "name": "Button"
        }
      },
      {
        "key": "ctrl+win+alt+u",
        "command": "trongate.insertSnippet",
        "when": "editorTextFocus",
        "args": {
          "name": "Button Alt"
        }
      },
      {
        "key": "ctrl+win+alt+c",
        "command": "trongate.insertSnippet",
        "when": "editorTextFocus",
        "args": {
          "name": "Contact Form"
        }
      },
      {
        "key": "ctrl+win+alt+d",
        "command": "trongate.insertSnippet",
        "when": "editorTextFocus",
        "args": {
          "name": "Download URL"
        }
      },
      {
        "key": "ctrl+win+alt+f",
        "command": "trongate.insertSnippet",
        "when": "editorTextFocus",
        "args": {
          "name": "Form"
        }
      },
      {
        "key": "ctrl+win+alt+g",
        "command": "trongate.insertSnippet",
        "when": "editorTextFocus",
        "args": {
          "name": "Grid"
        }
      },
      {
        "key": "ctrl+win+alt+i",
        "command": "trongate.insertSnippet",
        "when": "editorTextFocus",
        "args": {
          "name": "Info Page"
        }
      },
      {
        "key": "ctrl+win+alt+l",
        "command": "trongate.insertSnippet",
        "when": "editorTextFocus",
        "args": {
          "name": "Login Form"
        }
      },
      {
        "key": "ctrl+win+alt+m",
        "command": "trongate.insertSnippet",
        "when": "editorTextFocus",
        "args": {
          "name": "Modal"
        }
      },
      {
        "mac": "ctrl+win+alt+o",
        "key": "ctrl+win+alt+p",
        "command": "trongate.insertSnippet",
        "when": "editorTextFocus",
        "args": {
          "name": "Password Form"
        }
      },
      {
        "key": "ctrl+win+alt+t",
        "command": "trongate.insertSnippet",
        "when": "editorTextFocus",
        "args": {
          "name": "Table"
        }
      },
      {
        "key": "ctrl+win+alt+a",
        "command": "trongate.insertSnippet",
        "when": "editorTextFocus",
        "args": {
          "name": "Table Alt"
        }
      },
      {
        "key": "ctrl+win+alt+e",
        "command": "trongate.insertSnippet",
        "when": "editorTextFocus",
        "args": {
          "name": "Template"
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "jest"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.31.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "jest": "^26.6.1",
    "mocha": "^8.1.3",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.5",
    "vscode-test": "^1.4.0",
    "webpack": "^5.3.0",
    "webpack-cli": "^4.1.0"
  },
  "dependencies": {
    "@types/lodash": "^4.14.170",
    "@types/mkdirp": "^1.0.1"
  },
  "__metadata": {
    "id": "b082ed32-362d-47e2-b312-8504849483c9",
    "publisherId": "c4e42984-f95b-4460-8364-7dba22bc8b7f",
    "publisherDisplayName": "jc-sf"
  }
}
